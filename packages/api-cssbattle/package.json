    "cheerio": "^1.0.0-rc.10"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.30",
    "@types/node": "^16.6.1",
    "browserify": "^17.0.0",
    "gulp": "^4.0.2",
    "gulp-rimraf": "^1.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.12",
    "typescript": "^4.3.5"
  }
}