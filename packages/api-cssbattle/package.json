{
  "name": "api-cssbattle",
  "version": "1.0.0",
  "description": "Client api to obtain public information from the CSS Battle page",
  "license": "MIT",
  "private": false,
  "keywords": [
    "api",
    "cssbattle",
    "cssbattle-solutions",
    "nodejs",
    "typescript"
  ],
  "scripts": {
    "clean": "rimraf dist",
